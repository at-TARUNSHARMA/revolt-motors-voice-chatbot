{
  "version": 2,
  "functions": {
    "backend/server-live.js": {
      "maxDuration": 300
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/server-live.js"
    },
    {
      "src": "/health",
      "dest": "/backend/server-live.js"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/build/$1"
    }
  ]
}
